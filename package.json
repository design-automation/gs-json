{
    "name": "gs-json",
    "license": "GPL-3.0",
    "version": "0.1.7",
    "author": "Patrick Janssen",
    "description": "gs-JSON is a domain agnostic unifying 3D file format for geometric and semantic modelling (hence the 'gs').",
    "homepage": "https://github.com/phtj/gs-json#readme",
    "types": "dist/index.d.ts",
    "main": "dist/index.js",
    "directories": {
        "doc": "docs"
    },
    "scripts": {
        "build_three_models": "npm run build2015 && npm run gen_three_models",
        "build_gs_models": "npm run build2015 && npm run gen_gs_models",
        "build_models": "npm run build2015 && npm run gen_gs_models && npm run gen_three_models",
        "gen_three_models": "node ./dist2015/src/typescript/gs-json/utils/gen_three_models.js",
        "gen_gs_models": "node ./dist2015/src/typescript/gs-json/utils/gen_gs_models.js",
        "build2015": "tsc && babel --presets es2015 ./dist --out-dir ./dist2015",
        "prepare": "npm run build",
        "build": "tsc",
        "test": "karma start ./karma.conf.ts",
        "docs": "typedoc --out  ./docs/typescript ./src/typescript/gs-json/utils --module commonjs --theme minimal"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/phtj/gs-json.git"
    },
    "dependencies": {
        "@types/mathjs": "0.0.35",
        "@types/three": "^0.84.37",
        "mathjs": "^3.19.0",
        "three": "^0.89.0"
    },
    "devDependencies": {
        "@types/jasmine": "^2.8.3",
        "@types/node": "^8.5.7",
        "babel-cli": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "jasmine": "^2.8.0",
        "jasmine-core": "^2.8.0",
        "karma": "^2.0.0",
        "karma-chrome-launcher": "^2.2.0",
        "karma-cli": "^1.0.1",
        "karma-coverage-istanbul-reporter": "^1.3.3",
        "karma-html-reporter": "^0.2.7",
        "karma-jasmine": "^1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "karma-typescript": "^3.0.9",
        "karma-typescript-preprocessor": "^0.3.1",
        "typedoc": "^0.9.0",
        "typescript": "^2.6.1"
    },
    "bugs": {
        "url": "https://github.com/phtj/gs-json/issues"
    },
    "keywords": [
        "3d",
        "modelling",
        "geometry",
        "semantics"
    ]
}
